PORT=4000
MONGODB_URI=mongodb://localhost:27017/oci-price-calculator
OCI_TENANCY_OCID=ocid1.tenancy...
OCI_USER_OCID=ocid1.user...
OCI_FINGERPRINT=aa:bb:cc:...
OCI_PRIVATE_KEY=-----BEGIN RSA PRIVATE KEY-----\n...
OCI_REGION=us-ashburn-1
OCI_COMPARTMENT_OCID=ocid1.compartment...

# AWS S3
FINOPS_ACCESS_KEY_ID=
FINOPS_SECRET_ACCESS_KEY=
FINOPS_S3_BUCKET=my-billing-reports
FINOPS_S3_REGION=us-east-1        # optional, default: us-east-1
FINOPS_S3_PREFIX=CUR/my-report/   # optional

# OCI Object Storage
FINOPS_OCI_NAMESPACE=mytenancy
FINOPS_OCI_BUCKET=billing-exports
FINOPS_OCI_PREFIX=billing/        # optional
# (reuses OCI_TENANCY_OCID, OCI_USER_OCID, OCI_FINGERPRINT, OCI_PRIVATE_KEY, OCI_REGION)

# Amazon Textract — PDF invoice extraction via POST /api/documents/upload/textract
TEXTRACT_REGION=us-east-1
TEXTRACT_ACCESS_KEY_ID=your-access-key
TEXTRACT_SECRET_ACCESS_KEY=your-secret-key
# Optional: S3 bucket for Textract S3Object source
TEXTRACT_S3_BUCKET=your-invoice-bucket

# Gemini AI — PDF invoice extraction (default for /upload, explicit via /upload/gemini)
# Get your key at https://aistudio.google.com/app/apikey
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash